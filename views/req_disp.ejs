                <table class="table table-user-information">
                    <tbody>
                        <tr>
                            <td>RequestID</td>
                            <td>UserID</td>
                            <td>BloodBankID</td>
                            <td>blood_type</td>
                            <td> blood_component</td>
                            <td> status</td>
                            <td> area</td>
                            <td>date_time</td>
                            <td>units</td>
                        </tr>
                <% for (var i = 0; i < data.length; i++) { %>
                    <tr>
                       
                        <td><%=data[i].requestid%> </td>
                        <td><%=data[i].UserID%></td>
                        <td><%=data[i].BloodBankID%></td>
                        <td><%=data[i].blood_type%></td>
                        <td><%=data[i].blood_component%></td>
                        <td><%=data[i].status%></td>
                        <td><%=data[i].area%></td>
                        <td><%=data[i].date_time%></td>
